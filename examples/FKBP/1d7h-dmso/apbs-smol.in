##########################################################################
### 1D7H/DMSO BINDING ENERGY
### $Id$
###
### Please see APBS documentation (http://apbs.sourceforge.net/doc/) for
### syntax help.
##########################################################################

read 
  mol pqr complex.pqr 
  mol pqr dmso-min.pqr 
  mol pqr 1d7h-min.pqr
end

# COMPLEX -- SOLVATED STATE (FOCUSING)
elec name complex-solv-coarse
    mg-manual
    dime 65 65 65
    nlev 4
    grid 1.5 1.5 1.5
    gcent mol 1
    mol 1
    lpbe
    bcfl mdh
    ion 1 0.010 2.0
    ion -1 0.010 2.0
    pdie  2.0
    sdie 78.0
    srfm smol
    chgm spl0
    srad 0.0
    swin 0.3
    sdens 10.0
    temp 300
    gamma 0.105
    calcenergy total
    calcforce no
end
elec name complex-solv-fine
    mg-manual
    dime 65 65 65
    nlev 4
    grid 0.225 0.225 0.225
    gcent mol 2
    mol 1
    lpbe
    bcfl focus
    ion 1 0.010 2.0
    ion -1 0.010 2.0
    pdie  2.0
    sdie 78.0
    srfm smol
    chgm spl0
    srad 0.0
    swin 0.3
    sdens 10.0
    temp 300
    gamma 0.105
    calcenergy total
    calcforce no
end

