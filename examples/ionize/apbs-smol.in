# SOLVATION OF ACETIC ACID
print energy acetic-solv - acetic-ref end
# SOLVATION OF ACETATE
print energy acetate-solv - acetate-ref end
# SOLVATION OF PROTON
print energy proton-solv - proton-ref end
# TOTAL ENERGY
print energy acetate-solv - acetate-ref + proton-solv - proton-ref - acetic-solv + acetic-ref end

quit
