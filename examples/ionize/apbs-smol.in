read 
    mol pqr acetic-acid.pqr
    mol pqr acetate.pqr
    mol pqr proton.pqr
end

# Solvated acetic acid
elec name acetic-solv
    mg-auto
    nlev 4
    dime 65 65 65
    cglen 12 12 12
    fglen 6 6 6 
    fgcent mol 1
    cgcent mol 1
    mol 1
    lpbe
    bcfl mdh
    ion  1 0.150 2.0
    ion -1 0.150 2.0
    pdie 2.0
    sdie 78.0
    chgm spl0
    srfm smol
    srad 0
    swin 0.3
    temp 293.0
    gamma 0.105
    calcenergy total
    calcforce no
end
# Reference acetic acid
elec name acetic-ref
    mg-auto
    nlev 4
    dime 65 65 65
    cglen 12 12 12
    fglen 6 6 6
    cgcent mol 1
    fgcent mol 1
    mol 1
    lpbe
    bcfl mdh
    ion  1 0.000 2.0
    ion -1 0.000 2.0
    pdie 2.0
    sdie 2.0
    chgm spl0                             # Charge disc method (linear)
    srfm smol
    srad 0
    swin 0.3
    temp 293.0
    gamma 0.105
    calcenergy total
    calcforce no
end
