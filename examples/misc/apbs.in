read 
    mol pqr mache.pqr                   # Read molecule 1
end
elec
    mg-manual                          # Use the multigrid method
    dime 97 97 97                      # Grid dimensions (c 2^(l+1) +1)
                                       #   (in x, y, z)
    nlev 4                             # Number of levels in multilevel
                                       #   hierarchy (usually 'l' from above
                                       #   definition)
    glen 100 100 100
                                       # lengths OR the grid spacing
    gcent mol 1                        # This can either be 'mol n' to center
                                       #   on a specific molecule or (x, y, z)
                                       #   coordinates
    mol 1                              # Which molecule (1, 2, ...)
    lpbe                               # lpbe/npbe = linear/nonlinear PBE
    bcfl 1                             # Boundary condition flag
                                       #  0 => Zero
                                       #  1 => Single DH sphere
                                       #  2 => Multiple DH spheres
                                       #  4 => Focusing
    ion 1 0.150 2.0                    # Counterion declaration:
    ion -1 0.150 2.0                   #  ion <charge> <conc (M)> <radius>
    pdie 1.0                           # Solute dielectric
    sdie 78.54                         # Solvent dielectric
    chgm 0                             # Charge disc method (linear)
    srfm 1                             # Surface calculation method
                                       #  0 => Mol surface for epsilon;
                                       #       inflated VdW for kappa; no
                                       #       smoothing
                                       #  1 => As 0 with harmoic average
                                       #       smoothing
                                       #  2 => Cubic spline 
    srad 1.4                           # Solvent radius
    swin 0.3                           # Surface cubic spline window
    temp 298.15                        # Temperature
    gamma 0.105                        # Surface tension (in kJ/mol/A^2)
    calcenergy 1                       # Energy I/O (to stdout)
                                       #  0 => don't write out energy
                                       #  1 => write out total energy
                                       #  2 => write out total energy and all
                                       #       components
    calcforce 0                        # Atomic forces I/O (to stdout)
                                       #  0 => don't write out forces
                                       #  1 => write out net forces on molecule
                                       #  2 => write out atom-level forces
    write pot dx potential             # Potential I/O
    write sspl dx accessibility        # Solvent accessibility I/O
    write ndens dx ion-density         # Ion number density
    write qdens dx charge-density      # Ion charge density
    write edens dx energy-density      # Energy density
    write charge dx charge             # Biomolecule charge distribution
end

quit
