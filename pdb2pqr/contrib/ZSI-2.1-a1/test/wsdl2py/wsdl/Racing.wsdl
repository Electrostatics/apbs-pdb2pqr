         <wsdl:output message="tns:EventSummaryRaceInfoSoapOut" />
      </wsdl:operation>

      <wsdl:operation name="EventSummaryRunnerInfo">
         <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Event Summary Runner Information :- Provides runner information including the name of the jockey (Galloping), driver (Harness) and trainer (Greyhounds) including amendments, barrier and handicap for all races at a specified meeting.</documentation>

         <wsdl:input message="tns:EventSummaryRunnerInfoSoapIn" />

         <wsdl:output message="tns:EventSummaryRunnerInfoSoapOut" />
      </wsdl:operation>

      <wsdl:operation name="EventSummaryJDT">
         <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Event Summary (Jockey, Driver, Trainer) :- Provides jockey (Galloping), driver (Harness), trainer (Greyhounds) information, listed alphabetically by surname, including name, racenumber and runner number, for all races at a specified meeting.</documentation>

         <wsdl:input message="tns:EventSummaryJDTSoapIn" />

         <wsdl:output message="tns:EventSummaryJDTSoapOut" />
      </wsdl:operation>

