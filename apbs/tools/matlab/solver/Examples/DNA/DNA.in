   mg-auto
   mol 1
   ion charge 1 conc 2.0 radius 3.58 
   ion charge 2 conc 0.000 radius 4.28 
   ion charge -1 conc 2.0 radius 3.32
   cglen 200 200 200
   fglen 192 192 192
   dime 129 129 129
   cgcent mol 1 
   fgcent mol 1 
   bcfl mdh
   pdie 2.0
   sdie 2.0
   srfm smol
   chgm spl0
   srad 1.4
   swin 0.3
   smpbe vol 7 size 1000
   sdens 10
   temp 298.15
